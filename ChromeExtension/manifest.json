{
  "manifest_version": 3,
  "name": "YouTubeMusicDiscordRichPresence",
  "description": "Shows YouTubeMusic in Discord! By DevDenis",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://music.youtube.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "icons": {},
  "action": {},
  "content_security_policy": {},
  "host_permissions": [
    "http://localhost/*"
  ]
}